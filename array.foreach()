<html>
<body>
<h2>javaScript Arrays</h2>
<p id="demo"></p>
<script>
var fruits,text;
fruits=["banana","mango","kiwi","watermelon"];
text="<ul>";
fruits.forEach(myFunction);
text+="</ul>";
document.getElementById("demo").innerHTML=text;
function myFunction(value){
text+="<li>"+value+"</li>";
}
</script>
</body>
</html>
